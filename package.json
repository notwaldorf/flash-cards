{
  "name": "flash-cards",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "polymer serve --entrypoint index.html",
    "test": "mocha test/ --timeout=10000",
    "regen-test-baseline": "mocha test/screenshots-baseline/regenerate.js --timeout=10000",
    "old-build": "gulp copy",
    "bundle": "node ./hell/polymer-bundler/lib/bin/polymer-bundler.js --out-dir bundled --shell src/components/my-app.js src/components/my-app.js",
    "minify": "babel bundled --out-dir dist",
    "copy-polyfills": "cp -r node_modules/@webcomponents/webcomponentsjs ./dist",
    "copy-other-shit": "cp index.html ./dist/ && cp manifest.json ./dist/ && cp -r ./src/data ./dist/src/ && cp -r ./images ./dist/",
    "clean": "rm -rf bundled dist",
    "build": "npm run clean && npm run bundle && npm run minify && npm run copy-other-shit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notwaldorf/flash-cards.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notwaldorf/flash-cards/issues"
  },
  "homepage": "https://github.com/notwaldorf/flash-cards#readme",
  "dependencies": {
    "@0xcda7a/redux-es6": "^3.7.2",
    "@polymer/lit-element": "^0.2.2",
    "@webcomponents/webcomponentsjs": "^1.0.0",
    "gulp": "^3.9.1",
    "pwa-helpers": "polymerlabs/pwa-helpers",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "puppeteer": "^1.0.0",
    "pixelmatch": "^4.0.2",
    "polyserve": "^0.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-minify": "^0.2.0"
  }
}
